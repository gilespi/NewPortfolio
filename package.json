    "vue-resource": "^1.5.3",
    "vue-router": "^3.5.1",
    "vue-typer": "^1.2.0",
    "vueisotope": "^3.1.2",
    "vuetify": "^2.6.9",
    "vuetify-responsive-menu": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.19.1",
    "@mdi/font": "^7.0.96",
